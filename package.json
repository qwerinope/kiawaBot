{
  "dependencies": {
    "axios": "^1.7.9",
    "express": "^4.21.2",
    "jsonfile": "^6.1.0",
    "qs": "^6.13.0",
    "tesjs": "^1.0.7",
    "tmi.js": "^1.8.5",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  },
  "scripts": {
    "start": "node --env-file-if-exists .env start.js",
    "start:docker": "node start.js",
    "webserver": "node webserver/server.js",
    "webserver:dev": "nodemon webserver/server.js",
    "docker:build": "docker build -t kiara-bot .",
    "docker:run": "docker run -d --name kiara-bot -p 3000:3000 -p 8081:8081 --env-file .env -v ./data:/app/data kiara-bot",
    "docker:stop": "docker stop kiara-bot && docker rm kiara-bot",
    "docker:logs": "docker logs -f kiara-bot",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "compose:logs": "docker-compose logs -f",
    "compose:restart": "docker-compose restart"
  },
  "type": "module",
  "name": "kiara_bot",
  "version": "1.0.0",
  "main": "start.js",
